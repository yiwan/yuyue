}

.list {
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 20rpx 0;
  margin-bottom: 101rpx;
}

.form {
  box-shadow: 0rpx 0rpx 5rpx rgba(0, 0, 0, 0.5);
  width: 650rpx;
  border-radius: 10rpx;
}

.form+.form {
  margin-top: 20rpx;
}

.f-item {
  display: flex;
  height: 100rpx;
  align-items: center;
  padding: 0 20rpx;
}

.f-item +.f-item {
  border-top: 1px solid #ccc;
}

.f-label {
  width: 150rpx;
}

.f-input {
  flex: 1;
  height: 100%;
}

.btn-area {
  display: flex;
  align-items: center;
  justify-content: center;
}

.btn-add,.btn-adds {
  width: 670rpx;
  height: 90rpx;
  border-radius: 45rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #03a9f4;
}

.btn-submit{
  position: fixed;
  bottom: 100rpx;
  width: 750rpx;
}



